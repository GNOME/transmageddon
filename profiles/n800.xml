<?xml version="1.0"?>

<device>
    <make>Nokia</make>
    <model>N800</model>
    <description>profiles for Nokia N800</description>
    <author>
        <name>Christian Schaller</name>
        <email>christian.schaller@collabora.co.uk</email>
    </author>
    <version>1.0</version>
    <icon>file://ipod.svg</icon>
    <default>Normal</default>
    
    <profile>
        <name>Low</name> 
        <container>video/quicktime,variant=iso</container>
        <extension>mp4</extension>
        <audio>
            <name>audio/mpeg,mpegversion=[4, 2]</name>
            <presets>
                <preset>
                    Quality Low, Profile Low Complexity
                </preset>
            </presets>
            <channels>1, 2</channels>
            <samplerate>44100</samplerate>
        </audio>
        <video>
            <name>video/x-h264</name>
            <border>N</border>
            <passes>0</passes>
            <presets>
                <preset>
                    Quality Low, Profile Baseline
                </preset>
            </presets>
            <width>240, 352</width>
            <height>140, 208</height>
            <rate>1, 22</rate>
        </video>
    </profile>
    <profile>
        <name>Normal</name>
        <container>video/quicktime,variant=iso</container>
        <extension>mp4</extension>
        <audio>
            <name>audio/mpeg,mpegversion=[4, 2]</name>
            <presets>
                <preset>
                    Quality Normal, Profile Low Complexity
                </preset>
            </presets>
            <channels>1, 2</channels>
            <samplerate>44100</samplerate>
        </audio>
        <video>
            <name>video/x-h264</name>
            <border>N</border>
            <passes>0</passes>
            <presets>
                <preset>
                    Quality Normal, Profile Baseline
                </preset>
            </presets>
            <width>240, 352</width>
            <height>140, 208</height>
            <rate>1, 23</rate>
        </video>
    </profile>
</device>
