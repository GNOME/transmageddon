<?xml version="1.0"?>

<device>
    <make>Motorola</make>
    <model>KRZR K1v</model>
    <description>Profile for Motorola KRZR K1v</description>
    <author>
        <name>Christian Schaller</name>
        <email>uraeus@gnome.org</email>
    </author>
    <version>1.0</version>
    <icon>file://phone.svg</icon>
    <default>Normal</default>
    
    <profile>
        <name>Low</name> # This is a low quality output setting
        <container>video/quicktime,variant=3gpp</container>
        <extension>3gp</extension>
        <audio>
            <name>audio/mpeg,mpegversion=[4, 2]</name>
            <presets>
                <preset>
                    Quality Low, Profile Low Complexity
                </preset>
            </presets>
            <channels>1, 2</channels>
            <samplerate>44100</samplerate>
        </audio>
        <video>
            <name>video/mpeg,mpegversion=4</name>
            <border>N</border>
            <passes>0</passes>
            <presets>
                <preset>
                    Quality Low, Profile Simple
                </preset>
            </presets>
            <width>40, 165</width>
            <height>40, 200</height>
            <rate>1, 25</rate>
        </video>
    </profile>
    <profile>
        <name>Normal</name>
        <container>video/quicktime,variant=3gpp</container>
        <extension>3gp</extension>
        <audio>
            <name>audio/mpeg,mpegversion=[4, 2]</name>
            <presets>
                <preset>
                    Quality Normal, Profile Low Complexity
                </preset>
            </presets>
            <channels>1, 2</channels>
            <samplerate>44100</samplerate>
        </audio>
        <video>
            <name>video/mpeg,mpegversion=4</name>  
            <border>N</border>
            <passes>0</passes>
            <presets>
                <preset>
                    Quality Normal,Profile Simple
                </preset>
            </presets>
            <width>40, 165</width>
            <height>40, 200</height>
            <rate>1, 30</rate>
        </video>
    </profile>
</device>
