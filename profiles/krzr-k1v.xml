<?xml version="1.0"?>

<device>
    <make>Motorola</make>
    <model>KRZR K1v</model>
    <description>Profile for Motorola KRZR K1v</description>
    <author>
        <name>Christian Schaller</name>
        <email>uraeus@gnome.org</email>
    </author>
    <version>1.0</version>
    <icon>file://phone.svg</icon>
    <default>Normal</default>
    
    <profile>
        <name>Low</name> # This is a low quality output setting
        <container>video/quicktime,variant=3gpp</container>
        <extension>3gp</extension>
        <audio>
            <name>audio/AMR</name>
            <presets>
                <preset>
                    Quality Low
                </preset>
            </presets>
            <channels>1, 1</channels>
            <samplerate>8000</samplerate>
        </audio>
        <video>
            <name>video/mpeg,mpegversion=4</name>
            <border>Y</border>
            <passes>0</passes>
            <presets>
                <preset>
                    Quality Low, Profile Simple
                </preset>
            </presets>
            <width>176, 176</width>
            <height>144, 144</height>
            <framerate>1,15</framerate>
        </video>
    </profile>
    <profile>
        <name>Normal</name>
        <container>video/quicktime,variant=3gpp</container>
        <extension>3gp</extension>
        <audio>
            <name>audio/AMR</name>
            <presets>
                <preset>
                    Quality Normal
                </preset>
            </presets>
            <channels>1, 1</channels>
            <samplerate>8000</samplerate>
        </audio>
        <video>
            <name>video/mpeg,mpegversion=4</name>  
            <border>Y</border>
            <passes>0</passes>
            <presets>
                <preset>
                    Quality Normal,Profile Simple
                </preset>
            </presets>
            <width>176, 176</width>
            <height>144, 144</height>
            <framerate>1,15</framerate>
        </video>
    </profile>
</device>
