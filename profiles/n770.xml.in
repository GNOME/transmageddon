<?xml version="1.0"?>

<device>
    <make>Nokia</make>
    <model>N770</model>
    <_description>Profile for Nokia N770</_description>
    <author>
        <name>Christian Schaller</name>
        <email>christian.schaller@collabora.co.uk</email>
    </author>
    <version>1.0</version>
    <icon>file://ipod.svg</icon>
    <default>Normal</default>
    
    <preset>
        <_name>Low</_name> 
        <container>video/x-msvideo</container>
        <extension>avi</extension>
        <audio>
            <name>audio/mpeg,mpegversion=1,layer=3</name>
            <passes>
                <pass>
                    CBR Quality Normal, Profile Constant Bitrate
                </pass>
            </passes>
            <channels>1, 2</channels>
            <rate>44100</rate>
        </audio>
        <video>
            <name>video/mpeg,mpegversion=4</name>
            <passes>
                <pass>
                    Quality Low, Profile Advanced Simple
                </pass>
            </passes>
            <width>60, 352</width>
            <height>60, 208</height>
            <rate>1, 15</rate>
        </video>
    </preset>
    <preset>
        <_name>Normal</_name>
        <container>video/x-msvideo</container>
        <extension>avi</extension>
        <audio>
            <name>audio/mpeg,mpegversion=1,layer=3</name>
            <passes>
                <pass>
                    CBR Quality Low, Profile Constant Bitrate
                </pass>
            </passes>
            <channels>1, 2</channels>
            <rate>44100</rate>
        </audio>
        <video>
            <name>video/mpeg,mpegversion=4</name>
            <aspectratio>5:3</aspectratio>
            <passes>
                <pass>
                    Quality Low, Profile Simple
                </pass>
            </passes>
            <width>60, 352</width>
            <height>60, 208</height>
            <rate>1, 15</rate>
        </video>
    </preset>
</device>
