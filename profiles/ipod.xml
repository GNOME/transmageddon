<?xml version="1.0"?>

<device>
    <make>Apple</make>
    <model>iPod Nano</model>
    <description>Profile for Apple iPod Nano</description>
    <author>
        <name>Joe Example</name>
        <email>mr.example@example.org</email>
    </author>
    <version>1.0</version>
    <icon>file://ipod.svg</icon>
    <default>Normal</default>
    
    <profile>
        <name>Low</name>
        <container>video/quicktime,variant=apple</container>
        <extension>m4v</extension>
        <audio>
            <name>audio/mpeg,mpegversion=[4, 2]</name>
            <presets>
                <preset>
                    Quality Low, Profile Low Complexity
                </preset>
            </presets>
            <channels>1, 2</channels>
            <samplerate>44100</samplerate>
        </audio>
        <video>
            <name>video/x-h264</name>
            <border>Y</border>
            <passes>0</passes>
            <presets>
                <preset>
                    Quality Low, Profile Baseline
                </preset>
            </presets>
            <width>320, 640</width>
            <height>240, 480</height>
            <framerate>1, 25</framerate>
        </video>
    </profile>
    <profile>
        <name>Normal</name>
        <container>video/quicktime,variant=apple</container>
        <extension>m4v</extension>
        <audio>
            <name>audio/mpeg,mpegversion=[4, 2]</name>
            <presets>
                <preset>
                    Quality Normal, Profile Low Complexity
                </preset>
            </presets>
            <channels>1, 2</channels>
            <samplerate>44100</samplerate>
        </audio>
        <video>
            <name>video/x-h264</name>
            <border>Y</border>
            <passes>2</passes>
            <presets>
                <preset>
                    Quality Normal, Profile Baseline
                </preset>
            </presets>
            <width>320, 640</width>
            <height>240, 480</height>
            <framerate>1, 30</framerate>
        </video>
    </profile>
</device>
