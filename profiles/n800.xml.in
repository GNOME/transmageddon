<?xml version="1.0"?>

<device>
    <make>Nokia</make>
    <model>N800</model>
    <_description>Profile for Nokia N800</_description>
    <author>
        <name>Christian Schaller</name>
        <email>christian.schaller@collabora.co.uk</email>
    </author>
    <version>1.0</version>
    <icon>file://ipod.svg</icon>
    <default>Normal</default>
    
    <preset>
        <_name>Low</_name> 
        <container>video/quicktime,variant=iso</container>
        <extension>mp4</extension>
        <audio>
            <name>audio/mpeg,mpegversion=[4, 2]</name>
            <border>N</border>
            <passes>
                <pass>
                    Quality Low, Profile Low Complexity
                </pass>
            </passes>
            <channels>1, 2</channels>
            <samplerate>44100</samplerate>
        </audio>
        <video>
            <name>video/x-h264</name>
            <passes>
                <pass>
                    Quality Low, Profile Baseline
                </pass>
            </passes>
            <width>240, 352</width>
            <height>140, 208</height>
            <rate>1, 22</rate>
        </video>
    </preset>
    <preset>
        <_name>Normal</_name>
        <border>N</border>
        <container>video/quicktime,variant=iso</container>
        <extension>mp4</extension>
        <audio>
            <name>audio/mpeg,mpegversion=[4, 2]</name>
            <passes>
                <pass>
                    Quality Normal, Profile Low Complexity
                </pass>
            </passes>
            <channels>1, 2</channels>
            <samplerate>44100</samplerate>
        </audio>
        <video>
            <name>video/x-h264</name>
            <passes>
                <pass>
                    Quality Normal, Profile Baseline
                </pass>
            </passes>
            <width>240, 352</width>
            <height>140, 208</height>
            <rate>1, 23</rate>
        </video>
    </preset>
</device>
